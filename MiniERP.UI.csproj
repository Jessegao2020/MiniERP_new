    <None Remove="Assets\new.png" />
    <None Remove="Assets\order.png" />
    <None Remove="Assets\quotation.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\delete.png" />
    <Resource Include="Assets\duplicate.png" />
